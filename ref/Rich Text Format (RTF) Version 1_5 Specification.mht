  <TR align=3D"left" valign=3D"TOP">
    <TD><B>\pnb</B></TD>
    <TD>Bold numbering.*</TD></TR>
  <TR align=3D"left" valign=3D"TOP">
    <TD><B>\pni</B></TD>

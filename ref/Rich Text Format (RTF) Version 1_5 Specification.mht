  <TR align=3D"left" valign=3D"TOP">
    <TD><B>\pnscaps</B></TD>
    <TD>Small Caps numbering.*</TD></TR>
  <TR align=3D"left" valign=3D"TOP">
    <TD><B>\pnul</B></TD>
